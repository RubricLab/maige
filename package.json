    "bleed": "bun x npm-check-updates -u",
    "format": "bun x biome format --write .",
    "lint": "bun x biome check . && bun x biome lint .",
    "lint:fix": "bun x biome check --fix --unsafe . && bun x biome lint --write --unsafe .",
